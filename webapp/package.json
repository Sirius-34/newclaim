{
  "name": "@newclaim/webapp",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level=warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@nanostores/react": "^0.8.4",
    "@newclaim/backend": "workspace:*",
    "@newclaim/shared": "workspace:*",
    "@sentry/react": "^9.12.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "antd": "^5.25.4",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.64.0",
    "nanostores": "^0.11.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-image-gallery": "^1.4.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^7.2.0",
    "reset-css": "^5.0.2",
    "sass": "^1.85.0",
    "superjson": "^1.13.1",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.25.42",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^3.3.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.15",
    "@types/mixpanel-browser": "^2.54.0",
    "@types/node": "^22.15.24",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.14.0",
    "stylelint": "^16.14.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "vite": "^6.1.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
